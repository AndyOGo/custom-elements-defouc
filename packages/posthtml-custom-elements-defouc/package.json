{
  "name": "posthtml-custom-elements-defouc",
  "description": "A PostHTML plugin to mitigate intrinsic FOUC of Custom Elements.",
  "version": "1.0.0",
  "author": "PostHTML",
  "ava": {
    "verbose": "true"
  },
  "bugs": "https://github.com/AndyOGo/custom-elements-defouc/issues",
  "dependencies": {
    "posthtml": "^0.9.0",
    "posthtml-attrs-parser": "^0.1.1"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "mversion": "^1.10.1",
    "snazzy": "^4.0.0",
    "standard": "^7.1.2"
  },
  "engines": {
    "node": ">=4"
  },
  "homepage": "https://github.com/AndyOGo/custom-elements-defouc",
  "keywords": [
    "html",
    "posthtml",
    "posthtml-plugin",
    "posthtml-custom-elements",
    "custom-elements",
    "fouc"
  ],
  "license": "MIT",
  "main": "lib",
  "repository": "AndyOGo/custom-elements-defouc",
  "scripts": {
    "precommit": "npm run lint",
    "lint": "standard | snazzy",
    "release-major": "mversion major",
    "release-minor": "mversion minor",
    "release-patch": "mversion patch",
    "test": "npm run lint && ava"
  }
}
